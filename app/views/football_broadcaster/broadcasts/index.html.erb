<div class="page-header">
  <h1>Broadcasts</h1>
</div>

<%= link_to 'New broadcast', new_broadcast_path, class: 'btn btn-primary' %>

<h2>Ongoing broadcasts</h2>

<ul id="broadcasts">
  <% @broadcasts.each do |broadcast| %>
    <li>
      <%= link_to broadcast.title, broadcast_path(broadcast) %>
    </li>
  <% end %>
</ul>
